<!DOCTYPE html>
<html>
	<head>
		<title>Blogs</title>
		<%= stylesheet_link_tag    "application", :media => "all" %>
		<%= stylesheet_link_tag    "header", :media => "all" %>
		<%= stylesheet_link_tag    "footer", :media => "all" %>
		<%= javascript_include_tag "application" %>
		<%= csrf_meta_tags %>
	</head>
	<div id="container">
		<div id="header">
			<%= render 'layouts/header'%>
		</div>
		<div id="content">
			<%= yield %>
		</div>
		<div id="footer">
			<%= render 'layouts/footer'%>
		</div>
	</div>
	</body>
</html>